{
  "name": "main",
  "version": "1.0.0",
  "description": "Noted app for AI-powered note-taking",
  "license": "ISC",
  "author": "Harshit Jain <jharshit760@gmail.com>",
  "homepage": "https://github.com/yourusername/noted-app",
  "type": "commonjs",
  "main": "./main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron . --no-sandbox",
    "dist": "electron-builder -l"
  },
  "build": {
    "appId": "com.noted.notedapp",
    "productName": "Noted",
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Utility",
      "icon": "build/icons/png/1024x1024.png",
      "maintainer": "Harshit Jain <jharshit760@gmail.com>"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "files": [
      "**/*",
      {
        "from": "../frontend/dist",
        "to": "frontend/dist",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "@langchain/core": "^1.1.8",
    "@langchain/google-genai": "^2.1.3",
    "@langchain/langgraph": "^1.0.7",
    "dotenv": "^17.2.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "electron": "^26.2.1",
    "electron-builder": "^26.0.12"
  }
}